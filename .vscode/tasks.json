{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "lime",
			"command": "test",
			"group": "build"
		},
		{
			"type": "lime",
			"command": "build",
			"problemMatcher": [
				"$haxe-absolute",
				"$haxe",
				"$haxe-error",
				"$haxe-trace"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "never",
				"panel": "shared",
			}
		}
	]
}
